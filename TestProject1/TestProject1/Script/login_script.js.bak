function login(username, password)
{
  //Runs the "IndelPlanV2_0" tested application.
  TestedApps.IndelPlanV2_0.Run();
  //Clicks at point (60, 12) of the 'lineEdit_Username' object.
  Aliases.INDELP_1.logonClass.lineEdit_Username.Click(60, 12);
  //Enters in the 'lineEdit_Username' object.
  Aliases.INDELP_1.logonClass.lineEdit_Username.Keys(username);
  //Clicks at point (51, 11) of the 'lineEdit_Password' object.
  Aliases.INDELP_1.logonClass.lineEdit_Password.Click(51, 11);
  //Enters in the 'lineEdit_Password' object.
  Aliases.INDELP_1.logonClass.lineEdit_Password.Keys(password);
  //Clicks the 'pushButton' button.
  Aliases.INDELP_1.logonClass.pushButton.ClickButton();
  //Checks whether the 'QtText' property of the Aliases.INDELP_1.IntergratedGamaClass.centralWidget.stackedWidget.page.PatientManagementWidget.groupBox.frame.groupBox_3.label_UserName object equals 'User Name: our'.
  aqObject.CheckProperty(Aliases.INDELP_1.IntergratedGamaClass.centralWidget.stackedWidget.page.PatientManagementWidget.groupBox.frame.groupBox_3.label_UserName, "QtText", cmpEqual, "User Name: " + username);
}